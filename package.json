    "react-scripts": "0.8.5"
  },
  "dependencies": {
    "material-ui": "0.17.0",
    "mobile-detect": "^1.3.5",
    "mobx": "3.1.0",
    "mobx-react": "4.1.0",
    "mobx-router": "0.0.7",
    "node-static": "0.7.9",
    "per-env": "1.0.2",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-iframe": "0.0.4",
    "react-scripts": "0.8.5",
    "react-tap-event-plugin": "2.0.1",
    "sw-precache": "5.0.0"
  },
  "scripts": {
    "build": "react-scripts build && sw-precache --config=sw-precache-config.js",
